   var ctx = document.getElementById('myChart');\n      new Chart(ctx, {\n          type: 'bar',\n          data: {\n              labels: ['Red', 'Blue', 'Yellow', 'Green', 'Purple', 'Orange', 'black'],\n              datasets: [{\n                  label: '# Votos',\n                  data: [12, 19, 3, 5, 2, 3, 7],\n                  backgroundColor: bgColorBar,\n                //   backgroundColor: [\n                //       'rgba(255, 99, 132, 0.8)',\n                //       'rgba(54, 162, 235, 0.8)',\n                //       'rgba(255, 206, 86, 0.8)',\n                //       'rgba(75, 192, 192, 0.8)',\n                //       'rgba(153, 102, 255, 0.8)',\n                //       'rgba(255, 159, 64, 0.8)',\n                //       'rgba(0, 0, 0, 0.8)'\n                //   ],\n                  borderColor: [\n                      'rgba(255, 99, 132, 1)',\n                      'rgba(54, 162, 235, 1)',\n                      'rgba(255, 206, 86, 1)',\n                      'rgba(75, 192, 192, 1)',\n                      'rgba(153, 102, 255, 1)',\n                      'rgba(255, 159, 64, 1)',\n                      'rgba(0, 0, 0, 1)',\n                  ],\n                  borderWidth: 1\n              }]\n          },\n          options: {\n              scales: {\n                  y: {\n                      beginAtZero: true\n                  }\n              }\n          }\n      })\n    }\n  },\n  mounted(){\n    this.graph();\n  }\n}\n</script>\n\n<style scoped>\n#myChart{\n    max-width: 4000px;\n    max-height: 5000px;\n}\n</style>"],"sourceRoot":"src/components/_exibicaoDeObjetos/_Analytics/_graficos"}]}